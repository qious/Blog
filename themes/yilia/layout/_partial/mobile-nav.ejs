<nav id="mobile-nav">
  <div class="overlay">
    <div class="iconfont icon-deji slider-trigger"></div>
    <div class="header-author js-mobile-header fixed"><%=theme.author%></div>
  </div>
  <div class="intrude-less">
    <header id="header" class="inner">
      <div class="profilepic">
        <img src="<%=theme.avatar%>" <% if (theme.animate){ %>class="animate"<% } %>>
      </div>
      <hgroup>
        <div class="header-author"><%=theme.author%></div>
      </hgroup>
      <% if (theme.subtitle){ %>
      <p class="header-subtitle"><%=theme.subtitle%></p>
      <%}%>
      <nav class="header-menu">
        <ul>
        <% for (var i in theme.menu){ %>
          <li><a href="<%- url_for(theme.menu[i]) %>"><%= i %></a></li>
        <%}%>
        <div class="clearfix"></div>
        </ul>
      </nav>
      <nav class="header-nav">
        <div class="social">
          <% for (var i in theme.subnav){ %>
            <a class="<%= i %>" target="_blank" href="<%- url_for(theme.subnav[i]) %>" title="<%= i %>">
              <i class="iconfont icon-<%= i %>"></i>
            </a>
          <%}%>
        </div>
      </nav>
    </header>
  </div>
</nav>
